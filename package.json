{
	"name": "next.js-template",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start"
	},
	"dependencies": {
		"next": "9.5.3",
		"react": "16.13.1",
		"react-dom": "16.13.1"
	},
	"devDependencies": {
		"@types/node": "^14.10.0",
		"@types/react": "^16.9.49",
		"typescript": "^4.0.2"
	},
	"bit": {
		"env": {
			"compiler": {
				"bit.envs/compilers/react-typescript@3.1.63": {
					"rawConfig": {
						"tsconfig": {
							"compilerOptions": {
								"target": "es2016",
								"lib": [
									"dom",
									"dom.iterable",
									"esnext",
									"es6",
									"es2017"
								],
								"module": "CommonJS"
							}
						}
					}
				}
			}
		},
		"componentsDefaultDirectory": "components/{name}",
		"packageManager": "npm"
	}
}
